{
  "name": "tyt's tool box",
  "description": "Testing!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_title": "点击查看tyt's tool box详情",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": ["content.css"]
    }
  ]
}
